#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../can-bind-to-host/dist/bin/can-bind-to-host.js" "$@"
else 
  exec node  "$basedir/../can-bind-to-host/dist/bin/can-bind-to-host.js" "$@"
fi
